
public class Main {

	public static void main(String[] args) {
		System.out.println("hallo welt");
		Tier[] tier1= new Tier[5];
		Tier[] tier2= new Tier[5];
		int anzahl = 5;
		
		for (int i=0; i<anzahl; i++){
			tier1[i] = new Katze();
			tier2[i] = new Hund();
			
			tier1[i].gibLaut();
			tier2[i].gibLaut();
			
			tier1[i].laufen();
			tier2[i].laufen();
		}

	}

}
