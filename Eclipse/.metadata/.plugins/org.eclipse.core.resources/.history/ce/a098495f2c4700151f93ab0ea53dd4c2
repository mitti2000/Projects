
public class Katze extends Tier{
	
	public Katze (String name, int alter){
		this.name = name;
		this.alter = alter;
		tierart = "Katze";
	}

	@Override
	void laufen() {
		System.out.println(Die);
		
	}

	@Override
	void gibLaut() {
		String laut = "miau";
		System.out.println("Gibt laut " + laut);
		
	}
	

}
