import java.util.ArrayList;

public class Main {

	public static void main(String[] args) {
		ArrayList<Person> personen = new ArrayList<Person>();
		
		personen.add(new Mitarbeiter("Hans", "Müller", 5836, 80, 5300));
		personen.add(new Mitarbeiter("Fritz", "Meier", 5736, 100, 6300));
		personen.addAll(new Lehrling("Felix", "Fleixon", 15, 2, 4));

	}

}
