
public class Hund extends Tier{
	
	public Hund(String name, int alter){
		this.name = name;
		this.alter = alter;
		tierart="Hund";
	}
	@Override
	void laufen() {
		String laufen = "gallopierned";
		System.out.println("Läuft " + laufen);
		
	}

	@Override
	void gibLaut() {
		String laut = "WauWau";
		System.out.println("Gibt laut " + laut);
	}
}
