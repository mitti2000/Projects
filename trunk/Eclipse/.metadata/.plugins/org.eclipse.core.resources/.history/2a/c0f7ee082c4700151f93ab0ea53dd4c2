
public class Katze extends Tier{
	
	public Katze String name, int alter){
		this.name = name;
		this.alter = alter;
	}

	@Override
	void laufen() {
		String laufen = "elegant";
		System.out.println("Läuft " + laufen);
		
	}

	@Override
	void gibLaut() {
		String laut = "miau";
		System.out.println("Gibt laut " + laut);
		
	}
	

}
