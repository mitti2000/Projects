package ch.mitti.domev1;

public class Main {

	public static void main(String[] args) {
		
		CD cd1 = new CD("Dark side of the moon", "Pink Floyd", "10", "1h 10min");
		CD cd2 = new CD("Ignition", "The Offspring", "12", "58min");
		DVD dvd1 = new DVD("Ben Hur", "Who Knows?", "seehr lange");
		DVD dvd2 = new DVD("Matrix", "Wachowskys", "1h 40min");
		
		Datenbank db = new Datenbank();
		
		db.erfasseDisc(cd1);
		db.erfasseDisc(cd1);
		db.erfasseDisc(dvd1);
		db.erfasseDisc(dvd1);
		
		db.auflisten();
	}

}
