
public class Main {

	public static void main( String[] args ) {
		Animal sheep1 = new Animal();
		sheep1.getOlder();
		Animal sheep2 = new Animal();
		int age1 = sheep1.getOlder();
		int age2 = sheep2.getAge();
		System.out.println("Alter Schaf1:");
		System.out.println(age1);
		System.out.println("Alter Schaf2:");
		System.out.println(age2);
		System.out.println("warten");
		System.out.println("Zeit vergeht");
		System.out.println("und alles wird älter");
		age1 = sheep1.getOlder();
		age2 = sheep2.getOlder();
		System.out.println("Alter Schaf1:");
		System.out.println(age1);
		System.out.println("Alter Schaf2:");
		System.out.println(age2);
	  }
}
