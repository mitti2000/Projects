
public class Animal {
	private int age;
	private String name="";
	
	public Animal(String name){
		age = 0;
	}
	
	public void setName(String newName){
		name=newName;
	}
	
	public String getName(){
		return name;
	}
	
	public int getOlder(int oldAge){
		int newAge = oldAge+1;
		return newAge;
	}
	
	public int getAge(){
		return age;
	}
}
